@import "/app.wxss";
Swiper{
  height:340rpx;
}
.navigrtion{
  box-sizing: border-box;
  display: flex;
}
.navigrtion>navigator{
  flex-wrap: wrap;
  flex: 1;
}
.navigrtion>navigator>image{
  width: 100%;
  padding:20rpx;
}
/* 楼层部分 */
.tower-fuZhuang navigator{
  width:33.33%;
  float: left;
}
.tower-zhuTu image{
  width:100%;
  height: 100%;
}
.tower-fuZhuang navigator:nth-last-child(-n + 4){
  height:calc(750rpx / 3 * 386 / 232 / 2);
  padding:0 0 10rpx 10rpx;
}
.tower-fuZhuang{
  padding:10rpx;
}